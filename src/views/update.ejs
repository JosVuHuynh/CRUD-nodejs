<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css">
    <title>Update user</title>
</head>
<body>
    <div class="update-user-container">
        <form action="/update-user" method="POST">
            <div>
                <label >FirstName</label>
                <input name="firstName" value="<%= dataUser.firstName%>"/>
            </div>
            <div>
                <label >LastName</label>
                <input name="lastName" value="<%= dataUser.lastName%>"/>
            </div>
            <div>
                <label >Email</label>
                <input name="email" value="<%= dataUser.email%>"/>
            </div>
            <div>
                <label >Address</label>
                <input name="address" value="<%= dataUser.address%>"/>
            </div>
            <input type="text"  value="<%= dataUser.id %>" hidden name="id">
            <div>
                <button type="submit" class="btn-save">Save</button>
            </div>
        </form>
    </div>
  
</body>
</html>